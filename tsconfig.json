{
	"compilerOptions": {
		"baseUrl": "./src",
		"outDir": "./dist/",
		"lib": [
			"es2015",
			"es2016",
			"es6",
			"dom"
		],
		"target": "es2015",
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"preserveConstEnums": true,
		"importsNotUsedAsValues": "error",
		"allowSyntheticDefaultImports": true,
		"strict": true
	}
}
